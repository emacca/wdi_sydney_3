<%= form_for @artist do |f| %>

	<%= f.label :name %> <!-- when view source it shows artist[name] automatically done -->
	<%= f.text_field :name %><br>

	<%= f.label :nationality %>
	<%= f.text_field :nationality %><br>

	<%= f.label :dob %>
	<%= f.text_field :dob %><br>

	<%= f.label :period %>
	<%= f.text_field :period %><br>

	<%= f.label :image %>
	<%= f.text_field :image %><br>

	<%= f.submit  %>

<% end %>