<div>
    <form action="/history" method="get">
        <button>Display History!</button>
    </form>

    <form action="/display_movie_list" method="get">
        <button>Return to Display Movie List!</button>
    </form>

    <h1>Results</h1>
    <table border="1" cellpadding="10" cellspacing="10">
        <tr>
            <th>Title
            </th>
            <th>Poster
            </th>
        </tr>
        <%	if !File.file?("data/#{filename_new}.csv")	%>
		<%=		puts "No file exists with name: #{filename_new}" %>
		<%	elsif File.file?("data/#{filename_new}.csv")		
				CSV.foreach("data/#{filename_new}.csv") do |row|  
        %>
        <tr>
            <td> <%= puts row[0].to_s %>
            </td>
            <td> <img src="<%= puts row[1].to_s %>" width="100" height="100"> 
            </td>
        </tr>
        <% 
    			end
    		end
         %>
    </table>

    
</div>